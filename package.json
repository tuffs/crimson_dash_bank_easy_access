{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/turbo-rails": "^8.0.16",
    "@tailwindcss/cli": "^4.1.8",
    "jquery": "^3.7.1",
    "parsleyjs": "^2.9.2",
    "tailwindcss": "^4.1.8"
  },
  "scripts": {
